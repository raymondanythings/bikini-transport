{
  "name": "deus-ex-machina",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "panda codegen && yarn openapi:generate",
    "openapi:generate": "openapi-typescript openapi.yaml -o src/generated/api-types.ts",
    "dev": "vite",
    "build": "yarn openapi:generate && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ark-ui/react": "^5.27.0",
    "@biomejs/biome": "^2.2.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-mobile-picker": "^1.2.0",
    "react-router": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@pandacss/dev": "^1.4.3",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^4.0.9",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "msw": "^2.12.1",
    "openapi-msw": "^2.0.0",
    "openapi-typescript": "^7.10.1",
    "swagger-ui-watcher": "^2.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9"
  },
  "packageManager": "yarn@4.11.0",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
